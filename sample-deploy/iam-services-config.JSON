{
    "services": [
        {
            "name": "<Solution Container HostName>-Service",
            "url": "https://<IRIS_HOSTNAME>:<WEBGATEWAY_SECURE_PORT>",
            "routes": [
                {
                    "name": "API_Route_1",
                    "paths": [
                        "/<UNIQUE_INSTANCE_NAME_1>"
                    ],
                    "protocols": [
                        "https"
                    ],
                    "strip_path": true
                },
                {
                    "name": "API_Route_2",
                    "paths": [
                        "/<UNIQUE_INSTANCE_NAME_2>"
                    ],
                    "protocols": [
                        "https"
                    ],
                    "strip_path": true
                }
            ]
        },
        {
            "name": "<Solution Container HostName>-Service",
            "url": "https://<ANOTHER_IRIS_HOSTNAME>:<ANOTHER_WEBGATEWAY_SECURE_PORT>",
            "routes": [
                {
                    "name": "API_Route_3",
                    "paths": [
                        "/<UNIQUE_INSTANCE_NAME_3>"
                    ],
                    "protocols": [
                        "https"
                    ],
                    "strip_path": true
                }
            ]
        },
        {
            "name": "MonitorService",
            "url": "/api/monitor",
            "routes": [
                {
                    "name": "MetricsRoute",
                    "paths": [
                        "/metrics"
                    ],
                    "protocols": [
                        "https"
                    ],
                    "strip_path": false
                },
                {
                    "name": "AlertRoute",
                    "paths": [
                        "/alerts"
                    ],
                    "protocols": [
                        "https"
                    ],
                    "strip_path": false
                }
            ]
        }
    ]
}